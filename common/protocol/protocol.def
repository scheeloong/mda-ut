        INT_T,                  RW_T,    NULL, NULL)


///////////////////////// depth sensor
DEFDATA(CMD_DEPTH,
        FLOAT_T,                  RW_T,    NULL, NULL)

DEFDATA(CUR_DEPTH,
        FLOAT_T,                  RW_T,    NULL, NULL)

///////////////////////// motor drivers
DEFDATA(CMD_SIDE_SPEED,
        FLOAT_T,                  RW_T,    NULL, NULL)

DEFDATA(CMD_FWD_SPEED,
        FLOAT_T,                  RW_T,    NULL, NULL)


