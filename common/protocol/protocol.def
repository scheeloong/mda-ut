DEFDATA(CUR_IMAGE,
        IMG_T,                  RW_T,     pre_image_handler, post_image_handler)
///////////////////////// processed picture/image
DEFDATA(PROC_IMAGE,
        IMG_T,                  RW_T,     pre_image_handler, post_image_handler)

